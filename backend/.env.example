# Base de datos PostgreSQL
DB_HOST=localhost
DB_PORT=5432
DB_NAME=rodrigo_nap_db
DB_USER=postgres
DB_PASSWORD=your_password

# JWT
JWT_SECRET=your_super_secret_jwt_key_here
